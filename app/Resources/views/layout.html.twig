    <script>
        function getOccu(array, id) {
            var nbOcc = 0;
            for (var i = 0; i < array.length; i++) {
                if (array[i] == id) {
                    nbOcc++;
                }
            }
            return nbOcc
        }

        function updatepanier() {
            $("#cartBody").html("")
            $.ajax({
                url: "http://localhost/datatable_21/web/app_dev.php/home/getSession", success: function (result) {
                    var array = result.data
                    var notDupli = []
                    $.each(array, function (i, el) {
                        if ($.inArray(el, notDupli) === -1) notDupli.push(el);
                    });

                    $.each(notDupli, function (i, el) {

                        $.ajax({
                            async: true,
                            url: "http://localhost/datatable_21/web/app_dev.php/home/getProduct/" + el,
                            success: function (p) {

                                $("#cartBody").append('<tr>' +
                                    '<td><a href="#"><img alt="" src="http://localhost/datatable_21/web/' +
                                    p.image +
                                    '"></a></td>' +
                                    '<td><a href="#">' + p.nom + '</a></td>' +
                                    '<td>' + getOccu(array, p.id) + '</td>' +
                                    '<td>' + getOccu(array, p.id) * p.prix + ' dt</td>' +
                                    '<td><a class="close" href="#"><i class="fa fa-close font-13"></i></a></td>' +
                                    '</tr>');
                            }
                        })
                    });


                    $("#countCart").html("<i class='fa fa-shopping-cart font-13'></i> " + result.data.length)


                }
            });

        }

        updatepanier()


    </script>
